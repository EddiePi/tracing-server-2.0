<rules>
    <rule>
        <regex>.*Running task.* in (?&lt;stageID&gt;stage [0-9]+\.[0-9]).*(?&lt;TID&gt;TID [0-9]+).*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <!-- required -->
            <name>stage</name>
            <type>state</type>
            <tag>stageID</tag>
            <!-- optional, default: false -->
            <isFinish>false</isFinish>
            <value>1</value>
        </group>
        <group>
            <name>task</name>
            <type>event</type>
            <tag>TID</tag>
            <isFinish>false</isFinish>
            <value>1</value>
        </group>
    </rule>
    <rule>
        <regex>.*Finished task.* in (?&lt;stageID&gt;stage [0-9]+\.[0-9]).*(?&lt;TID&gt;TID [0-9]+).*\s(?&lt;byte&gt;[0-9]+).*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <!-- required -->
            <name>stage</name>
            <type>state</type>
            <tag>stageID</tag>
            <!-- optional, default: false -->
            <isFinish>false</isFinish>
            <value>1</value>
        </group>
        <group>
            <name>task</name>
            <type>event</type>
            <tag>TID</tag>
            <isFinish>true</isFinish>
            <value>1</value>
        </group>
        <group>
            <name>byte.transferred</name>
            <type>event</type>
            <isFinish>true</isFinish>
            <value>byte</value>
        </group>
    </rule>
    <rule>
        <regex>.*stored as (?:values|bytes) in memory.*estimated size (?&lt;byte&gt;[0-9]+\.[0-9]+.*),.*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>byte.stored</name>
            <type>event</type>
            <isFinish>true</isFinish>
            <value>byte</value>
        </group>
    </rule>
</rules>